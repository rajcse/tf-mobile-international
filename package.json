{
  "name": "truthfinder-mobile",
  "version": "1.0.0",
  "description": "TruthFinder Mobile App",
  "author": "The Control Group",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "android": "cordova run android",
    "android:build": "cordova build --release --buildConfig=./build.json android -- --gradleArg=-PcdvVersionCode=1001592273",
    "build": "NODE_ENV=production webpack --display-error-details",
    "build:dev": "NODE_ENV=development webpack --display-error-details",
    "ios": "cordova run ios",
    "clean": "node config && cordova clean",
    "prepare": "node config && NODE_ENV=production webpack && cordova prepare",
    "compile": "node config && NODE_ENV=production webpack && cordova build",
    "start": "node config && NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 3000",
    "start:prod": "node config && NODE_ENV=production webpack-dev-server --host 0.0.0.0 --port 3000",
    "lint": "eslint app && yarn outdated",
    "test": "NODE_ENV=production cross-env NODE_PATH=./app mocha --compilers js:babel-core/register -w test/utils/browser.js test/*.spec.js"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chart.js": "^2.2.0-rc.1",
    "classnames": "^2.2.5",
    "config": "^1.24.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "es6-promise": "^3.3.1",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.1",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "flux": "^2.1.1",
    "graceful-fs": "^4.1.9",
    "inline-environment-variables-webpack-plugin": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.0.1",
