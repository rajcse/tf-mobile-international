    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-test-utils": "^15.4.0",
    "react-chartjs-2": "^1.1.7",
    "react-dom": "^15.2.0",
    "react-owl-carousel": "^0.14.0",
    "react-router": "^2.4.1",
    "react-router-scroll": "^0.3.2",
    "react-router-transition": "0.0.6",
    "react-scroll": "^1.4.4",
    "react-sticky": "^5.0.4",
